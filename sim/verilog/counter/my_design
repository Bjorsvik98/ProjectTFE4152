    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x56469ab4dd10_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56469ab4dd10_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x56469ab4dd10_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56469ab3ab00;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56469ab4e010_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56469ab4df40_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x56469ab3ab00;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56469ab4e010_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56469ab4e010_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56469ab4e010_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56469ab4e010_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x56469ab3ab00;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x56469ab4df40_0;
    %nor/r;
    %store/vec4 v0x56469ab4df40_0, 0, 1;
